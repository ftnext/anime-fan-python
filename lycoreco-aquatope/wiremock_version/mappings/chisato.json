                "equalTo": "application/json"
            }
        },
        "bodyPatterns": [
            {
                "equalToJson": {
                    "lines": "sakana"
                }
            }
        ]
    },
    "response": {
        "status": 200,
        "bodyFileName": "chisato.json"
    }
}
